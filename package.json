{
  "name": "usecasegen",
  "version": "1.0.0",
  "description": "Clean Architecture Use Case Generator for TypeScript projects",
  "main": "lib/index.js",
  "bin": {
    "usecasegen": "bin/usecasegen.js",
    "generate-usecase": "bin/generate-usecase.js"
  },
  "readme": "npm-README.md",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "npm run clean && mkdir -p bin lib && cp src/bin/* bin/ && cp -r src/lib/* lib/ && chmod +x bin/*.js",
    "clean": "rm -rf bin lib",
    "prepublishOnly": "npm run build && npm test",
    "postpublish": "echo 'Published successfully to npm!'",
    "prepare": "echo 'Package is ready for use!'"
  },
  "keywords": [
    "clean-architecture",
    "usecase",
    "boilerplate",
    "generator",
    "typescript",
    "domain-driven-design",
    "dependency-injection",
    "nextjs"
  ],
  "author": "Calebe Machado",
  "email": "calebe@clsax.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clsax/usecasegen.git"
  },
  "bugs": {
    "url": "https://github.com/Clsax/usecasegen/issues"
  },
  "homepage": "https://github.com/Clsax/usecasegen#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "readline": "^1.3.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1"
  }
}
